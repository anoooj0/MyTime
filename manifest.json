{
  "manifest_version": 3,
  "name": "MyTime",
  "description": "Take control of your time by blocking unproductive sites.",
  "version": "0.0.1",
  "permissions": ["storage", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
    // "default_icon": {
    //   "16": "icon16.png",
    //   "48": "icon48.png",
    //   "128": "icon128.png"
  }
}
